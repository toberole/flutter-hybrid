<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.zw.mylibrary2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Androidflutter">
        <activity android:name=".activity1.RecycleViewActivity"></activity>
        <activity android:name=".activity1.ViewModelActivity" />
        <activity android:name=".activity1.LiveDataActivity" />
        <activity android:name=".activity1.MyLifecycleRegistryActivity" />
        <activity android:name=".activity1.MyLifecycleActivity" />
        <activity android:name=".activity1.GlideActivity" />
        <activity android:name=".activity1.DiskLruCacheActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>